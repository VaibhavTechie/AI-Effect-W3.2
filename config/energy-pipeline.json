{
  "workflow_name": "energy_data_pipeline",
  "start_node": "energy-generator",
  "execution_mode": "grpc",
  "service_registry": {
    "energy-generator": "localhost:50051",
    "energy-analyzer": "localhost:50052",
    "report-generator": "localhost:50053"
  },
  "containers": {
    "energy-generator": {
      "id": "energy-generator",
      "input_file": "/data/energy_data.csv",
      "output_file": "/data/output1.pb",
      "next_node": "energy-analyzer",
      "timeout_seconds": 30
    },
    "energy-analyzer": {
      "id": "energy-analyzer",
      "input_file": "/data/output1.pb",
      "output_file": "/data/output2.pb",
      "next_node": "report-generator",
      "timeout_seconds": 60
    },
    "report-generator": {
      "id": "report-generator",
      "input_file": "/data/output2.pb",
      "output_file": "/data/energy_report.csv",
      "next_node": null,
      "timeout_seconds": 30
    }
  }
}