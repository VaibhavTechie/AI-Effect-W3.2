{
  "workflow_name": "energy_data_pipeline_corrected",
  "start_node": "energy-generator",
  "execution_mode": "grpc",
  "service_registry": {
    "energy-generator": "77.237.247.251:50051",
    "energy-analyzer": "77.237.247.251:50052",
    "report-generator": "77.237.247.251:50053"
  },
  "containers": {
    "energy-generator": {
      "id": "energy-generator",
      "input_file": "/data/placeholder_input.txt",
      "output_file": "/data/energy_data.csv",
      "next_node": "energy-analyzer",
      "timeout_seconds": 30
    },
    "energy-analyzer": {
      "id": "energy-analyzer",
      "input_file": "/data/energy_data.csv",
      "output_file": "/data/analysis_output.pb",
      "next_node": "report-generator",
      "timeout_seconds": 60
    },
    "report-generator": {
      "id": "report-generator",
      "input_file": "/data/analysis_output.pb",
      "output_file": "/data/energy_report.csv",
      "next_node": null,
      "timeout_seconds": 30
    }
  }
}